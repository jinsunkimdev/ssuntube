extends base
block content
	if(!loggedIn)
		h4 Login please
	ul
		each userInfo in users 
			h3 UserName👩‍🏫 = #{userInfo.userName}
			h5 UserID = #{userInfo.userId} 
			h5 Password = #{userInfo.password} 
			h5 AvatarUrl = #{userInfo.avatarUrl}
			h6=userInfo.createdAt 
			hr
		each videoInfo in videos
			h3 VideoTitle📺 = #{videoInfo.videoTitle}
			a(href=`/videos/${videoInfo._id}`, rel="stylesheet") link
			h5 Description = #{"/" + videoInfo.description}
			h5 Video File Url = #{videoInfo.fileUrl}
			h5 Created By = #{videoInfo.createdBy}
			video(controls, width="250", height="200")
				source(src=videoInfo.fileUrl, type="video/mp4")
			hr